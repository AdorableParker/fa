use fa;



class Program {
	public static void Main () {
		int aaa = 10;
		string _s = switch {
			when aaa == 3      => "A",
			when aaa == 5      => "B",
			when aaa == 8      => "C",
			when aaa == 10     => "D",
			when aaa == 15     => "E",
			_                  => "F",
		};
		Console.WriteLine (_s);
	}
}

